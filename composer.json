{
    "name": "jonnyw/craft-awss3assets",
    "description": "A Craft CMS plugin allowing you to upload assets to an AWS S3 bucket.",
    "keywords": ["Craft", "AWS", "S3", "Assets"],
	"type": "craft-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonny Wenmoth",
            "email": "contact@jonnyw.me",
            "homepage": "http://www.jonnyw.me"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JonnyW\\AWSS3Assets\\": "src/JonnyW/AWSS3Assets/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "~1.0",
        "aws/aws-sdk-php": "~3.19"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/phpunit-bridge": "~3.1"
    }
}
